@model IEnumerable<QuanLyDiemSinhVien.Models.KhoaHoc>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Thêm mới khóa học", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MaKhoaHoc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TenKhoaHoc)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.MaKhoaHoc)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.TenKhoaHoc)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.MaKhoaHoc }) |
        @Html.ActionLink("Details", "Details", new { id = item.MaKhoaHoc }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.MaKhoaHoc })
    </td>
</tr>
}

</table>
